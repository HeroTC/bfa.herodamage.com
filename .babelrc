{
  "plugins": [
    "@lingui/babel-plugin-transform-js",
    "@lingui/babel-plugin-transform-react"
  ],
  "env": {
    "extract": {
      "presets": [
        "babel-preset-react"
      ],
      "plugins": [
        "babel-plugin-transform-object-rest-spread",
        "babel-plugin-transform-class-properties"
      ]
    }
  }
}
